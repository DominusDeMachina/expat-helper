# Supabase Configuration
# Copy this file to .env in your project root and replace with your actual values

# Supabase Project URL (found in Settings > API)
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anonymous Key (found in Settings > API)
# This is safe to use in client-side code
EXPO_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_anon_key_here

# Supabase Service Role Key (found in Settings > API)
# Keep this secret! Only use on server-side or in secure environments
# Don't use EXPO_PUBLIC prefix for this one
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_service_role_key_here

# Optional: Custom Supabase configuration
# SUPABASE_JWT_SECRET=your_jwt_secret_here
# SUPABASE_DATABASE_PASSWORD=your_database_password_here 